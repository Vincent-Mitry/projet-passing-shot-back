<form method="post" action="{{ path('app_court_delete', {'id': court.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce terrain ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ court.id) }}">
    <button class="btn btn-danger m-3">Supprimer</button>
</form>
